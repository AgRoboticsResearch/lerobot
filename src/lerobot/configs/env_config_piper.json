{
  "env": {
    "type": "gym_manipulator",
    "name": "real_robot",
    "fps": 10,
    "processor": {
      "control_mode": "gamepad",
      "observation": {
        "display_cameras": true
      },
      "image_preprocessing": {
        "crop_params_dict": {},
        "resize_size": [128, 128]
      },
      "gripper": {
        "use_gripper": true,
        "gripper_penalty": 0.0
      },
      "reset": {
        "reset_time_s": 5.0,
        "control_time_s": 20.0,
        "terminate_on_success": true
      },
      "inverse_kinematics": {
        "urdf_path": "/home/hls/codes/lerobot/piper_description/urdf/piper_description.urdf",
        "target_frame_name": "gripper_base",
        "end_effector_bounds": {
          "min": [-0.2, -0.2, 0.0],
          "max": [0.2, 0.2, 0.2]
        },
        "end_effector_step_sizes": {
          "x": 0.02,
          "y": 0.02,
          "z": 0.02
        }
      }
    },
    "robot": {
      "type": "piper",
      "can_interface": "can0",
      "use_degrees": true,
      "include_gripper": false
    },
    "teleop": {
      "type": "gamepad",
      "use_gripper": true
    }
  },
  "dataset": {
    "repo_id": "username/piper_task",
    "root": null,
    "task": "piper_task",
    "num_episodes_to_record": 10,
    "replay_episode": 0,
    "push_to_hub": true
  },
  "mode": "record",
  "device": "cuda"
}


